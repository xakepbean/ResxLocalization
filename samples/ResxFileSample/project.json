{
  "dependencies": {
    "Microsoft.AspNetCore.Diagnostics": "1.1.0-*",
    "Microsoft.AspNetCore.Mvc": "1.1.0-*",
    "Microsoft.AspNetCore.Mvc.Formatters.Json": "1.1.0-*",
    "Microsoft.AspNetCore.Mvc.Localization": "1.1.0-*",
    "Microsoft.AspNetCore.Mvc.Razor": "1.1.0-*",
    "Microsoft.AspNetCore.Mvc.TagHelpers": "1.1.0-*",
    "Microsoft.AspNetCore.Razor.Tools": {
      "version": "1.0.0-preview2-final",
      "type": "build"
    },
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.0-*",
    "Microsoft.AspNetCore.StaticFiles": "1.1.0-*",
    "Microsoft.Extensions.Logging": "1.1.0-*",
    "Microsoft.Extensions.Logging.Console": "1.1.0-*",
    "Microsoft.Extensions.Logging.Debug": "1.1.0-*",
    "Microsoft.Extensions.DependencyInjection": "1.1.0-*",
    "Newtonsoft.Json": "9.0.1",
    "System.Xml.XDocument": "4.0.*",
    "Xakep.AspNetCore.Localization": "1.0.0-*",
    "Xakep.Extensions.Localization": "1.0.0-*",
    "Microsoft.Extensions.Configuration": "1.1.0-*",
    "Microsoft.Extensions.Configuration.FileExtensions": "1.1.0-*",
    "Microsoft.Extensions.Configuration.Json": "1.1.0-*"
  },

  "tools": {
    "Microsoft.AspNetCore.Razor.Tools": "1.0.0-preview2-final"
  },

  "frameworks": {
    "net451": {},
    "netcoreapp1.0": {
      "dependencies": {
        "System.Text.Encoding.CodePages": "4.0.1",
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      },
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },
  "publishOptions": {
    "include": [
      "wwwroot",
      "Views",
      "Areas/**/Views",
      "appsettings.json",
      "web.config"
    ]
  }
}
